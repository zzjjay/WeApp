.touch-item {
font-size: 14px;
display: flex;
background-color: white;
border:0.5rpx solid #d9d9d9;
border-bottom: 0.5rpx solid #d9d9d9 ;
width: 100%;
height:52px;
overflow: hidden;
margin: 0 0 15px 0;
border-radius: 5px;
}

.picture{
  margin-top: 1rpx;
  width: 60rpx;
  height: 60rpx;
  padding-right: 8px;
}

.weui-cells weui-cells_after-title{

margin: 0 0 15px 0;
border-radius: 5px;
border:0.5rpx solid #d9d9d9;

}
.content {
width:100%;
padding: 0px;
line-height: 25px;
margin-right:0;
-webkit-transition: all 0.4s;
transition: all 0.4s;
-webkit-transform: translateX(90px);
transform: translateX(90px);
margin-left: -90px
}

.weui-btn{
  background-color:#FFB5C5;
  position:fixed;
  bottom:20px;
  left:35px;
   width:80%;
  height:120rpx;
  font-size: 22px;
}